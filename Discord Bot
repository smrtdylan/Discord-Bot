        await message.channel.send(f' <@{lead_id}> has been mentioned.This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == 'bozo':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == 'L':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == "Didn't ask":
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == 'ratioed':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == "L+Bozo+Ratio+Didn't Ask+Don't Care+ Get a Life":
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == "Yadadgay":
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')


    if message.content == '!password8':
        import random
        import string
        lower = string.ascii_lowercase
        upper = string.ascii_uppercase
        number = '0123456789'
        symbols = '!@#$%^&*()_-+={[}]:;<,>.?/'
        join = str(lower + upper + number + symbols)
        password8 = "".join(random.sample(join, int(8)))
        await message.channel.send(f'Your password is: {password8}')
    elif message.content == '!password9':
        import random
