    #rockpaperscissors
    if message.content == '!playrock':
        import random
        ai_list = ['Rock', 'Paper', 'Scissors']
        random_index = random.randint(0,len(ai_list)-1)
        ai_choice = ai_list[random_index]
        if ai_choice == 'Rock':
            await message.channel.send(f'AI picks {ai_choice} you tied')
        elif ai_choice == 'Scissors':
            await message.channel.send(f'AI picks {ai_choice} you win!')
        elif ai_choice == 'Paper':
            await message.channel.send(f'AI picks {ai_choice} you lose')
    elif message.content == '!playpaper':
        import random
        ai_list = ['Rock', 'Paper', 'Scissors']
