import discord

bot = discord.Client()

@bot.event

async def on_ready():

    guild_count = 0

    for guild in bot.guilds:
        print(f"-{guild.id} (name: {guild.name})")

        guild_count = guild_count + 1
    print('guild count: ' + str(guild_count))
    print('Bot activated')

@bot.event
async def on_message(message):
    import random
    ai_list = ['hello!', 'hi!', 'howdy!', 'greetings!']
    lead_id = '822603581156753458'


    if message.content == ('hi'):
        await message.channel.send(random.choice(ai_list))
    elif message.content == 'hello':
        await message.channel.send(random.choice(ai_list))
    elif message.content == 'howdy':
        await message.channel.send(random.choice(ai_list))
    elif message.content == 'greetings':
        await message.channel.send(random.choice(ai_list))
    # toxicish words
    if message.content == 'i hate u':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. More behavior like this will end up with serious consequences.')
    elif message.content == 'f u':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == 'wtf':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == 'shut up':
        await message.channel.send(f' <@{lead_id}> has been mentioned. This is a warning. Behavior like this will end up with serious consequences.')
    elif message.content == 'furry':
